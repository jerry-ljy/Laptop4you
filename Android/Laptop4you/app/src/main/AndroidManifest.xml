<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.laptop4you">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_laptop"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Laptop4you">
        <activity android:name=".ShowResultActivity" />
        <activity android:name=".QueryPriceActivity" />
        <activity android:name=".QueryWorkActivity" />
        <activity android:name=".QueryUseActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QueryGameActivity" />
        <activity android:name=".QueryHomeActivity" />
    </application>

</manifest>